protocol Greetable {
	let name: String
}

protocol Greeter {
	func greet(name: String) -> String
}

struct Person: Greeter {
	let name: String

	func greet(name: String) -> String {
		"Oh hi, " + name
	}
}

struct Animal: Greetable {
	let name: String
}

let count = 100

let j = 0
loop j < count {
	let people = [Person(name: "Pat")]
	let animals  = [Animal(name: "Pooch")]

	let i = 0
	loop i < count {
		i = i + 1
		people.push(Person(name: "Person "))
		animals.push(Animal(name: "Animal"))
	}

	i = 0
	loop i < count {
		let person = people.get(i)
		let animal = animals.get(i)
		person.greet(name: animal.name)
		i = i + 1
	}

	print("all done with j")
	print(j)

	j = j + 1
}